{
  "name": "smolverse-inventory",
  "version": "1.0.0",
  "description": "Subgraph that powers inventory for Smolverse collections",
  "license": "MIT",
  "scripts": {
    "codegen": "graph codegen",
    "deploy:dev": "graph deploy --product hosted-service treasureproject/smolverse-inventory-dev",
    "deploy:prod": "graph deploy --product hosted-service treasureproject/smolverse-inventory",
    "prepare:dev": "yarn --cwd ../../packages/constants prepare:arbitrum-goerli && mustache ../../node_modules/@treasure/subgraph-config/src/arbitrum-goerli.json template.yaml > subgraph.yaml",
    "prepare:prod": "yarn --cwd ../../packages/constants prepare:arbitrum && mustache ../../node_modules/@treasure/subgraph-config/src/arbitrum.json template.yaml > subgraph.yaml",
    "test": "graph test -v 0.5.0"
  }
}
